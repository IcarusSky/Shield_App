<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/clItem"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingBottom="5dp"
    android:paddingLeft="8dp"
    android:paddingRight="8dp"
    android:paddingTop="5dp">
    <!--android:background="@color/white"-->
    <TextView
        android:id="@+id/tvItemPlaceholder"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:text="1111111"
        android:lines="1"
        android:textSize="11sp"
        android:visibility="invisible"/>

    <TextView
        android:id="@+id/tvItem"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:ellipsize="end"
        android:gravity="center"
        android:lines="1"
        android:text="11111"
        android:textColor="@color/text_com_color"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="@id/tvItemPlaceholder"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@id/tvItemPlaceholder"
        app:layout_constraintRight_toRightOf="@id/tvItemPlaceholder"
        app:layout_constraintTop_toTopOf="@id/tvItemPlaceholder"
        app:layout_constraintVertical_bias="0.0" />
</android.support.constraint.ConstraintLayout>